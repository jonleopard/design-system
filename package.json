{
  "name": "jonleopard-design-system",
  "version": "0.0.3",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 8000 -c .storybook",
    "storybook": "build-storybook -c .storybook -o _site/storybook",
    "build": "yarn icons && rm -rf _site && build-storybook -c .storybook -o _site",
    "chromatic": "chromatic test --exit-zero-on-changes --storybook-addon --app-code=cehrtj2764k --script-name start",
    "icons": "node scripts/parse-icons",
    "fmt": "prettier {.storybook,scripts,src,dist}/**/*.js --single-quote --no-semi --write"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "camelcase": "^5.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.1",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "recompose": "^0.30.0",
    "storybook-chromatic": "^1.0.2",
    "styled-components": "^3.4.5",
    "svgi": "^1.1.0"
  },
  "dependencies": {
    "clean-tag": "^2.0.0",
    "lodash": "^4.17.10",
    "palx": "^1.0.3",
    "prop-types": "^15.6.2",
    "rc-slider": "^8.6.1",
    "styled-system": "^3.0.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --single-quote --write",
      "git add"
    ]
  }
}
